{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@services/*": ["services/*"],
      "@routes/*": ["routes/*"]
    }
  },
  "include": ["**/*"],
  "ts-node": {
   "esm": true,
   "experimentalSpecifierResolution": "node"
},
}
